{
  "name": "poi-plugin-tsundb",
  "version": "0.0.7",
  "description": "TsunDB data submission plugin for Poi",
  "keywords": [
    "kancolle",
    "poi",
    "poi-plugin",
    "tsundb"
  ],
  "license": "MIT",
  "author": "gakada (https://github.com/gakada)",
  "contributors": [
    "sorewachigauyo (https://github.com/sorewachigauyo)"
  ],
  "homepage": "https://github.com/kcwiki/poi-plugin-tsundb#readme",
  "bugs": "https://github.com/kcwiki/poi-plugin-tsundb/issues",
  "repository": "kcwiki/poi-plugin-tsundb",
  "main": "dist/index.js",
  "files": [
    "dist",
    "i18n"
  ],
  "scripts": {
    "format": "prettier --loglevel warn --write '**/*.{js,json,md,ts,yaml,yml}'",
    "lint": "eslint . --ext ts --fix",
    "build": "rimraf dist && tsc",
    "test": "yarn format && yarn lint && yarn build && cross-env TSUNDB_API_URL=http://localhost:12345 tape -r ts-node/register test",
    "prepack": "yarn test",
    "up": "yarn upgrade --latest"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fastify": "^2.15.1",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "tape": "^5.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "poiPlugin": {
    "title": "Plugin.Title",
    "description": "Plugin.Description",
    "icon": "fa/pie-chart",
    "priority": 100
